{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "generate-secrets": "cd authentication && node generate-secrets.js && cd..",
    "deploy-data": " cross-env PROCESSTOSTART=restoredatabases node ./datasets/deploy-data",
    "deploy-data-dynamodb": " cross-env PROCESSTOSTART=restoredynamodb node ./datasets/deploy-data",
    "deploy-data-postgresql": " cross-env PROCESSTOSTART=restorepostgresql node ./datasets/deploy-data",
    "remove-data": " cross-env PROCESSTOSTART=removedata node ./datasets/deploy-data",
    "remove-tables": " cross-env PROCESSTOSTART=removetables node ./datasets/deploy-data",
    "test-deployment": "cd testing && npm i && node cypressrun.js && cypress open && cd.."
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "2.17.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.17.0",
    "aws-sdk": "^2.1096.0",
    "constructs": "^10.0.0",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "source-map-support": "^0.5.16"
  }
}
